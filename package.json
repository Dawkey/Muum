{
  "name": "muum",
  "version": "1.0.1",
  "main": "main.js",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "chokidar": "^3.6.0",
    "electron-store": "^8.1.0",
    "gsap": "^3.12.5",
    "hidefile": "^3.0.0",
    "lockfile": "^1.0.4",
    "music-metadata": "^7.14.0",
    "p-limit": "^3.1.0",
    "proper-lockfile": "^4.1.2",
    "react-fast-marquee": "^1.6.5",
    "three": "^0.108.0",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000/ && npm run electron\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prepack": "npm run build"
  },
  "build": {
    "appId": "Muum",
    "productName": "Muum",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "output": "dist"
    },
    "extends": null,
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "src/utils/spine"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@szhsin/react-menu": "^4.1.0",
    "classnames": "^2.5.1",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "^28.1.4",
    "electron-builder": "^24.13.3",
    "fuse.js": "^7.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.70.0",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
  }
}
